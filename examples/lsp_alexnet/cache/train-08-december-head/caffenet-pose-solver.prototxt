net: "caffenet-pose-lsp-train-val-multable-fc.prototxt"
test_iter: 10
test_interval: 500
base_lr: 0.0001
# The learning rate policy
lr_policy: "step"
gamma: 0.75
stepsize: 7000
display: 20
max_iter: 200000
momentum: 0.9
weight_decay: 0.0005
snapshot: 1000
snapshot_prefix: "pose_caffenet_train"
solver_mode: GPU
